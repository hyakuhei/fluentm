		graph [color=red fontname=Arial fontsize=12 label="Tiaga Co" line=dotted]
		subgraph cluster_Databases {
			graph [color=red fontname=Arial fontsize=12 label=Databases line=dotted]
			"Content DB"
		}
	}
	subgraph cluster_Logistics {
		graph [color=red fontname=Arial fontsize=12 label=Logistics line=dotted]
		"Order Resolver"
	}
	subgraph cluster_Warehouse {
		graph [color=red fontname=Arial fontsize=12 label=Warehouse line=dotted]
		"Label Printer"
	}
	subgraph cluster_Warehouse {
		graph [color=red fontname=Arial fontsize=12 label=Warehouse line=dotted]
		"Warehouse Notifier"
	}
	Customer -> "Web Server"
	"Web Server" -> "Content DB"
	"Web Server" -> "Stock DB"
	"Web Server" -> "Merchant API"
	"Order Resolver" -> "Label Printer"
	"Order Resolver" -> "Warehouse Notifier"
